<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ValueBar">
        <attr name="radius" format="dimension" />
        <attr name="level" format="integer" />
        <attr name="maxLevel" format="integer" />
        <attr name="minDotRadius" format="dimension" />
        <attr name="maxDotRadius" format="dimension" />
        <attr name="dotBackgroundColor" format="color" />
        <attr name="dotProgressColor" format="color" />
        <attr name="dotTextColor" format="color" />
        <attr name="showLevelText" format="boolean" />
    </declare-styleable>
    <declare-styleable name="AudioRecordingView">
        <attr name="recordingMode" format="enum">
            <enum name="recordPlayback" value="0" />
            <enum name="playbackOnly" value="1" />
        </attr>
    </declare-styleable>
</resources>